{
  "name": "typescript-react-ssr",
  "version": "0.5.0",
  "description": "A Boilerplate for Modern Web apps With Best development experience & best practices",
  "main": "./server/index.ts",
  "scripts": {
    "start": "nodemon ./index.ts",
    "webpack:server": "webpack ./tools/webpack/server/webpack.config.js",
    "start:prod": "cross-env NODE_ENV=production node ./build/server/index.js",
    "build": "npm-run-all clean -p build:*",
    "build:client": "cross-env NODE_ENV=production npm-run-all compile:client && webpack --config ./tools/webpack/client/webpack.config.js",
    "build:server": "cross-env NODE_ENV=production npm-run-all compile:server",
    "compile:client": "cross-env NODE_ENV=production BABEL_ENV=node babel ./app -d ./build/app --extensions '.ts,.tsx'",
    "compile:server": "cross-env NODE_ENV=production BABEL_ENV=node babel ./server -d ./build/server --extensions '.ts,.tsx'",
    "storybook": "start-storybook -p 6006 -c tools/storybook",
    "storybook:build": "build-storybook -o build/storybook-static",
    "format": "prettier --write \"./**/**/*.{js,ts,tsx}\"",
    "lint": "eslint . --ext .ts,.tsx",
    "clean": "rimraf build .awcache",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/AlaguSelvan/typescript-react-ssr.git"
  },
