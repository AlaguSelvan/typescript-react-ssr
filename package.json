{
  "name": "typescript-react-ssr",
  "version": "1.0.0",
  "description": "A Boilerplate for Modern Web apps With Best development experience & best practices",
  "main": "index.ts",
  "scripts": {
    "start": "cross-env NODE_ENV=development nodemon ./server/server.ts",
    "start:prod": "cross-env NODE_ENV=production node ./build/server/server.js",
    "build": "npm-run-all clean build:*",
    "build:server": "cross-env NODE_ENV=production BABEL_ENV=node babel ./server -d ./build/server --extensions '.ts,.tsx'",
    "build:live": "cross-env NODE_ENV=production BABEL_ENV=node babel ./app -d ./build/app --extensions '.ts,.tsx'",
    "build:client": "cross-env NODE_ENV=production webpack --config ./tools/webpack/webpack.config.js",
    "build:storybook": "build-storybook -o build/storybook-static",
    "storybook": "start-storybook -p 6006 -c tools/storybook",
    "deploy:storybook": "npm run build:storybook && storybook-to-ghpages",
    "format": "prettier --write \"./**/**/*.{ts,tsx}\"",
    "lint": "eslint . --ext .ts,.tsx",
    "clean": "rimraf build"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/AlaguSelvan/typescript-react-ssr.git"
  },
